        "    while n > 0:\n",
        "        print(n)\n",
        "        n -= 1  # Correct increment\n",
        "    print(\"Blastoff!\")\n",
        "\n",
        "# Test the function with an initial value\n",
        "countdown(10)\n",
        "\n"
      ]
